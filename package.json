{
  "name": "typescript-killer",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "awesome-typescript-loader": "^3.4.0",
    "babel-core": "^6.26.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  },
  "scripts": {
    "start": "time npx tsc --logLongCheckerCalls --logLongTimeThreshold 300 --logCheckerShortcut --lib \"es5\" index.ts",
    "slow": "time npx tsc --lib \"es5\" index.ts",
    "fast": "time npx tsc --logCheckerShortcut --lib \"es5\" index.ts",
    "watch:awesome": "npx webpack --watch --config webpack.config.js",
    "watch:tsc": "npx tsc --outDir ./tsc-build --watch --lib \"es5\" index.ts"
  }
}
